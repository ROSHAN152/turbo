pipeline {
  agent any
  stages {
    stage('Checkout') {
      steps {
        echo 'checkout scm'
      }
    }
    stage('Build') {
      steps {
        echo 'building..'
      }
    }
    stage('Test') {
      steps {
        echo 'testing...'
      }
    }
    stage('Deploy') {
      steps {
        echo 'deploying...'
      }
    }
  }
}